<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:XMR.HomeApp"
             x:Class="XMR.HomeApp.Pages.WebPage">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout Orientation="Horizontal">
                <Button Text="Go" Clicked="NavigateToPage" />
                <Entry x:Name="urlEntry" HorizontalOptions="FillAndExpand" />
            </StackLayout>
            <WebView x:Name="webView" Source="https://skillfactory.ru/" VerticalOptions="FillAndExpand" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>